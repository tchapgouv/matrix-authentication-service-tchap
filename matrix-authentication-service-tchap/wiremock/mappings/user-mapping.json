{
  "name": "User Standard Mapping",
  "request": {
    "method": "GET",
    "urlPathPattern": "/_matrix/identity/api/v1/internal-info",
    "queryParameters": {
      "medium": {
        "equalTo": "email"
      },
      "address": {
        "contains": "tchapgouv.com"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "hs": "tchapgouv.com",
      "requires_invite": false,
      "invited": false
    },
    "headers": {
      "Content-Type": "application/json"
    }
  },
  "priority": 100
}
